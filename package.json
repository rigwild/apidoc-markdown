{
  "name": "apidoc-markdown",
  "version": "7.2.1",
  "description": "📝 Generate a Simple and Portable Markdown documentation for your API",
  "license": "MIT",
  "repository": "rigwild/apidoc-markdown",
  "author": {
    "name": "rigwild",
    "email": "me@rigwild.dev",
    "url": "https://rigwild.dev"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates",
    "pnpm-lock.yaml"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "help": "node bin/index.js -h",
    "example:generate": "cd example && node generateExamples.js",
    "example:clean": "rm -rf example/basic/example.md example/multi/*",
    "test": "tsc && ava",
    "test:ava": "ava",
    "test:gen-apidoc": "cd test/_apidoc && rm -rf out && apidoc -i src -o out",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "apidoc-light": "^0.53.0",
    "ejs": "^3.1.8",
    "semver": "^7.3.7",
    "update-notifier": "^5.1.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/node": "^18.7.18",
    "@types/semver": "^7.3.12",
    "@types/yargs": "^17.0.12",
    "ava": "^4.3.3",
    "fs-extra": "^10.1.0",
    "typescript": "^4.8.3"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "portable",
    "api",
    "doc",
    "documentation",
    "generator",
    "programmatic",
    "cli",
    "cli-app",
    "command-line",
    "rest",
    "restful",
    "api-documentation",
    "markdown-documentation"
  ],
  "ava": {
    "files": [
      "test/**/*",
      "!test/_testFiles"
    ],
    "verbose": true
  }
}
