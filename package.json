{
  "name": "apidoc-markdown",
  "version": "5.2.2",
  "description": "ðŸ“ Generate API documentation in Markdown from apiDoc data",
  "license": "MIT",
  "repository": "rigwild/apidoc-markdown",
  "author": {
    "name": "rigwild",
    "email": "me@rigwild.dev",
    "url": "https://rigwild.dev"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "help": "node bin/index.js -h",
    "example:generate": "node example/generateExamples.js",
    "example:clean": "rm -rf example/basic/example.md example/multi/*",
    "test": "tsc && ava",
    "test:ava": "ava",
    "test:gen-apidoc": "cd test/_apidoc && rm -rf out && apidoc -i src -o out",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "semver": "^7.3.4",
    "update-notifier": "^5.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/node": "^14.14.35",
    "@types/semver": "^7.3.4",
    "@types/yargs": "^16.0.0",
    "apidoc": "^0.27.1",
    "ava": "^3.15.0",
    "fs-extra": "^9.1.0",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "portable",
    "api",
    "doc",
    "documentation",
    "generator",
    "programmatic",
    "cli",
    "cli-app",
    "command-line",
    "rest",
    "restful"
  ],
  "ava": {
    "files": [
      "test/**/*",
      "!test/_apidoc"
    ],
    "verbose": true
  }
}
