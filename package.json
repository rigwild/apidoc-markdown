{
  "name": "apidoc-markdown",
  "version": "7.3.0",
  "description": "📝 Generate a Simple and Portable Markdown documentation for your API",
  "license": "MIT",
  "repository": "rigwild/apidoc-markdown",
  "author": {
    "name": "rigwild",
    "email": "me@rigwild.dev",
    "url": "https://rigwild.dev"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates",
    "pnpm-lock.yaml"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "help": "node bin/index.js -h",
    "example:generate": "cd example && node generateExamples.js",
    "example:clean": "rm -rf example/basic/example.md example/multi/*",
    "test": "tsc && ava",
    "test:ava": "ava",
    "test:gen-apidoc": "cd test/_apidoc && rm -rf out && apidoc -i src -o out",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "apidoc-light": "^0.54.0",
    "ejs": "^3.1.9",
    "semver": "^7.5.4",
    "update-notifier": "^7.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.10.4",
    "@types/semver": "^7.5.6",
    "@types/yargs": "17.0.32",
    "ava": "^6.0.1",
    "fs-extra": "^11.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "portable",
    "api",
    "doc",
    "documentation",
    "generator",
    "programmatic",
    "cli",
    "cli-app",
    "command-line",
    "rest",
    "restful",
    "api-documentation",
    "markdown-documentation"
  ],
  "ava": {
    "files": [
      "test/**/*",
      "!test/_testFiles"
    ],
    "verbose": true
  }
}
