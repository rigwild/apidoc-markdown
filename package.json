{
  "name": "apidoc-markdown",
  "version": "5.1.8",
  "description": "ðŸ“ Generate API documentation in Markdown from apiDoc data",
  "license": "MIT",
  "repository": "rigwild/apidoc-markdown",
  "author": {
    "name": "rigwild",
    "email": "me@rigwild.dev",
    "url": "https://rigwild.dev"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "help": "node bin/index.js -h",
    "example:apidoc": "cd example/_apiDocData && rm -rf out && apidoc -i src -o out",
    "example:generate": "ts-node example/generateExamples.ts",
    "example:clean": "rm -rf example/basic/example.md example/multi/output/",
    "test": "ava --verbose",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "semver": "^7.3.2",
    "update-notifier": "^4.1.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/node": "^14.11.1",
    "@types/semver": "^7.3.4",
    "@types/yargs": "^15.0.5",
    "apidoc": "^0.25.0",
    "ava": "^3.12.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "portable",
    "api",
    "doc",
    "documentation",
    "generator",
    "programmatic",
    "cli",
    "cli-app",
    "command-line",
    "rest",
    "restful"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
